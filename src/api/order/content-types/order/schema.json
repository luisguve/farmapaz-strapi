{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "orderDate": {
      "type": "datetime"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Pendiente",
        "En revision",
        "Pagada",
        "En proceso",
        "Entregada",
        "Cancelada"
      ]
    },
    "deliveryDate": {
      "type": "datetime"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "orders"
    },
    "subscription": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::subscription.subscription",
      "inversedBy": "orders"
    },
    "productsDetails": {
      "type": "json"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "totalAmount": {
      "type": "decimal"
    },
    "reference": {
      "type": "string"
    },
    "paymentMethod": {
      "type": "enumeration",
      "enum": [
        "Divisa",
        "Efectivo",
        "Transferencia bancaria",
        "Pago movil"
      ]
    },
    "deliveries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::delivery.delivery",
      "mappedBy": "orders"
    }
  }
}
